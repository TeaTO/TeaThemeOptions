{
  "name": "crewstyle/tea-theme-options",
  "type": "wordpress-plugin",
  "description": "The Tea Theme Options library allows you to easily add professional looking theme options panels to your WordPress theme.",
  "keywords": [
    "wordpress",
    "plugin",
    "teathemeoptions",
    "themeoptions",
    "tea",
    "tto",
    "teato",
    "theme",
    "options"
  ],
  "homepage": "https://github.com/crewstyle/TeaThemeOptions",
  "license": "MIT",
  "authors": [
    {
      "name": "Achraf Chouk",
      "homepage": "https://github.com/crewstyle"
    }
  ],
  "require": {
    "ruflin/Elastica": "1.x",
    "twig/twig": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "crewstyle\\TeaThemeOptions\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  }
}
